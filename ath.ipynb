   "outputs": [],
   "source": [
    "temp = []\n",
    "for i in range(len(separate_at)-1):\n",
    "    if separate_at[i+1]-separate_at[i] > 1:\n",
    "        temp.append(separate_at[i])\n",
    "\n",
